   "source": [
    "def title_similarity(basisMovieID, checkedMovieID):    \n",
    "    # The higher value is the more similar (from 0 to 1) \n",
    "        \n",
    "    counter = 0\n",
    "    if basisMovieID in title_dict: \n",
    "        basisTags = title_dict[basisMovieID]\n",
    "        countAllTags = len(basisTags)\n",
    "        basisTagsDict = {}\n",
    "        for x in basisTags:\n",
    "            if x in basisTagsDict:\n",
    "                basisTagsDict[x] += 1\n",
    "            else:\n",
    "                basisTagsDict[x] = 1   \n",
    "        \n",
    "        for x in basisTagsDict:\n",
    "            basisTagsDict[x] = basisTagsDict[x] / countAllTags\n",
    "    else: \n",
    "        return 0\n",
    "    \n",
    "    \n",
    "    if checkedMovieID in title_dict: \n",
    "        checkedTags = title_dict[checkedMovieID]\n",
    "        checkedTags = set(checkedTags) \n",
    "        checkedTags = list(checkedTags)\n",
    "    else: \n",
    "        return 0\n",
    "    \n",
    "    \n",
    "    for x in basisTagsDict:\n",
    "        if x in checkedTags:\n",
    "            counter += basisTagsDict[x]\n",
    "    return counter"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 25,
   "metadata": {},
   "outputs": [],
   "source": [
    "genresSimilarityWeight = 0.2\n",
    "actorsSimilarityWeight = 0.4\n",
    "directSimilarityWeight = 0.2\n",
    "titleSimilarityWeight = 0.4\n",
    "yearGapWeight = 0.9                  # Can be tweaked to get the better recommnedation\n",
    "\n",
    "\n",
    "def checksimi(movieId):\n",
    "    basis_genre = np.array(list(df[df['movie_id']==movieId]['genresMatrix']))\n",
    "    basis_actor = np.array(list(df[df['movie_id']==movieId]['actor_matrix']))\n",
    "    basis_year = df[df['movie_id'] == movieId]['title_year']\n",
    "    basis_director = np.array(list(df[df['movie_id']==movieId]['director_matrix']))\n",
    "    \n",
    "    dff = df\n",
    "    dff['similarity'] = dff.apply(lambda x: \n",
    "                        spatial.distance.cosine(x['genresMatrix'],basis_genre)*genresSimilarityWeight  + spatial.distance.cosine(x['actor_matrix'],basis_actor)*actorsSimilarityWeight\n",
    "                        + spatial.distance.cosine(x['director_matrix'],basis_director)*directSimilarityWeight - title_similarity(x['movie_id'],movieId)*titleSimilarityWeight \n",
    "                        + abs(basis_year - x['title_year'])/100*yearGapWeight , axis=1)\n",
    "    \n",
    "    return dff[['movie_id','movie_title','title_year','director_name','actors','genres','similarity']].sort_values('similarity')"
   ]
  },
